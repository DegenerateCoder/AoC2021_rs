struct BingoBoard {
    board: BingoNumber,
    won: bool,
    
    // Nested structure declaration
    struct BingoNumber {
        number: u32,
        marked: bool,
    }
}


pub fn part1(file_path: &str) -> u32 {
    let file = std::fs::read_to_string(file_path).unwrap();
    let mut file = file.lines();

    let mut depth_increases = 0;
    let mut old_depth: u32 = file.next().unwrap().parse().unwrap();
    for line in file {
        let currrent_depth: u32 = line.parse().unwrap();
        if currrent_depth > old_depth {
            depth_increases += 1;
        }
        old_depth = currrent_depth;
    }

    depth_increases
}
